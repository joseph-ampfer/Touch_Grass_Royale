{
  "expo": {
    "name": "Touch Grass Royale",
    "description": "Getting off the phone to get on the podium.",
    "slug": "touch_grass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundImage": "./assets/adaptive-icon-bg (2).png",
        "monochromeImage": "./assets/monochrome5.png"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": ["android.permission.PACKAGE_USAGE_STATS"],
      "package": "com.jampfer.touch_grass",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "742c1b23-0c80-4edd-9f8c-6451ac3a3420"
      }
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Touch Grass Royale to access your contacts."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to select your profile picture."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Touch Grass Royale to use Face ID."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification.png",
          "color": "#2564eb"
        }
      ]
    ]
  }
}
